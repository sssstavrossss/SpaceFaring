<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Space Faring - Sky is the beggining</title>
    <link rel="shortcut icon" type="image/jpg" href="~/images/SpaceFaringLogoWeb.png" />
    @Styles.Render("~/AdminContent/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    @Html.Partial("_AdminNavBar")

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Space Faring</p>
        </footer>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/datatables")
    @Scripts.Render("~/bundles/moment")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/datetimePicker")
    @RenderSection("scripts", required: false)

</body>
</html>
