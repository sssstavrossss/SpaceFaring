<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Space Faring - Sky is the beggining</title>
    <link rel="shortcut icon" type="image/jpg" href="~/images/SpaceFaringLogoWeb.png" />
    @Styles.Render("~/AdminContent/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/custom/AdminPlanetDetails.css" rel="stylesheet" />
</head>
<body>

    @Html.Partial("_AdminNavBar")

    <div class="container-fluid body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Space Faring</p>
        </footer>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/datatables")
    @Scripts.Render("~/bundles/moment")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/datetimePicker")
    @RenderSection("scripts", required: false)

</body>
</html>
